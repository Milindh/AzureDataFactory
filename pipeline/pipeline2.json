{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "LookupTable",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.00:10:00",
					"retry": 2,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": {
							"value": "SELECT \n    t.name AS table_name, \n    SCHEMA_NAME(t.schema_id) AS schema_name\nFROM sys.tables t\nWHERE t.type = 'U' \n  AND SCHEMA_NAME(t.schema_id) = 'SalesLT';",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "ds_Azure_SQL",
						"type": "DatasetReference",
						"parameters": {
							"schema_name": {
								"value": "@pipeline().parameters.sname",
								"type": "Expression"
							},
							"table_name": {
								"value": "@pipeline().parameters.tname",
								"type": "Expression"
							}
						}
					},
					"firstRowOnly": false
				}
			}
		],
		"parameters": {
			"sname": {
				"type": "string",
				"defaultValue": "SalesLT"
			},
			"tname": {
				"type": "string",
				"defaultValue": "Customer"
			}
		},
		"variables": {
			"tablename": {
				"type": "String"
			}
		},
		"annotations": []
	}
}